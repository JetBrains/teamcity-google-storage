<?xml version="1.0" encoding="UTF-8"?>


<beans xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns="http://www.springframework.org/schema/beans"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
       default-autowire="constructor">
    <bean class="jetbrains.buildServer.serverSide.artifacts.google.GoogleStorageType"/>
    <bean class="jetbrains.buildServer.serverSide.artifacts.google.GoogleArtifactContentProvider"/>
    <bean class="jetbrains.buildServer.serverSide.artifacts.google.cleanup.GoogleCleanupExtension"/>
    <bean class="jetbrains.buildServer.serverSide.artifacts.google.web.GoogleSettingsController"/>
    <bean class="jetbrains.buildServer.serverSide.artifacts.google.web.GoogleArtifactDownloadProcessor"/>
    <bean class="jetbrains.buildServer.serverSide.artifacts.google.web.GoogleSignedUrlController"/>
    <bean class="jetbrains.buildServer.serverSide.artifacts.google.signedUrl.GoogleSignedUrlProviderImpl"/>
</beans>