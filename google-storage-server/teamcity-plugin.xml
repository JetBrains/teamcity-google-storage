<?xml version="1.0" encoding="UTF-8"?>


<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                 xsi:noNamespaceSchemaLocation="urn:shemas-jetbrains-com:teamcity-plugin-v1-xml">
    <info>
        <name>google-storage</name>
        <display-name>Google Artifact Storage</display-name>
        <version>@Plugin_Version@</version>
        <description>Allows storing build artifacts in a Google storage bucket</description>
        <download-url>https://github.com/JetBrains/teamcity-google-storage</download-url>
        <vendor>
            <name>JetBrains</name>
            <url>http://www.jetbrains.com/</url>
        </vendor>
    </info>
    <requirements min-build="46654" />
    <deployment use-separate-classloader="true" node-responsibilities-aware="true"/>
</teamcity-plugin>